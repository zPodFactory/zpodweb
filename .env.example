# Default target name shown in the login form
ZPODWEB_DEFAULT_ZPODFACTORY_NAME=zPodFactory

# Default zpodapi URL pre-filled in the login form
ZPODWEB_DEFAULT_ZPODFACTORY_API_URL=http://172.16.0.10:8000

# Default zpodapi access token pre-filled in the login form
ZPODWEB_DEFAULT_ZPODFACTORY_API_TOKEN=

# Port to expose zpodweb on (dev server and Docker)
ZPODWEB_DEFAULT_UI_PORT=8500

# Enable vSphere SOAP API debug logging (colored XML request/response)
DEBUG_API_VSPHERE=false

# Enable NSX REST API debug logging (colored JSON request/response)
DEBUG_API_NSX=false
